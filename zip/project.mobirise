ay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    <input type=\"hidden\" name=\"aiName\" value=\"footer_1\">\n  <input type=\"hidden\" name=\"aiDescription\" value=\"page footer with links and a copyright line\">\n  <input type=\"hidden\" name=\"aiStatus\" value=\"false\">\n  </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-7\" mbr-if=\"showTitle\">\n                <div class=\"title-wrapper\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\">Couch Token</h2>\n                </div>\n            </div>\n\t\t\t<div class=\"col-12 col-lg-5\" mbr-if=\"showNav\">\n                <div class=\"nav-wrapper\">\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list, .item-wrap, .nav-wrapper\" data-multiline=\"\" mbr-article=\"\">\n                        <li class=\"item-wrap\">Home</li>\n                        <li class=\"item-wrap\">About Couch</li>\n                        <li class=\"item-wrap\">Join Us</li>\n                        <li class=\"item-wrap\">Couch Events</li>\n                        <li class=\"item-wrap\">Contact Us</li>\n                    </ul>\n                </div>\n\t\t\t</div>\n            <div class=